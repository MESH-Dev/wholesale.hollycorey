	position:relative;
	transition:.1s linear all;
	transition:.1s linear all;
	display:block;
}

a.gl-slide:before {
	padding:0 5px;
	content:attr(title);
	position:absolute;
	left:0;
	top:100%;
	background:#8e44ad;
	color:#fff;
	transition:.1s linear all;
	transition:.1s linear all;
}

a.gl-slide:hover span,a.gl-slide:hover:before {
	transform:translateY(-100%);
}

a.gl-3d {
	position:relative;
	overflow:hidden;
	padding:0 5px;
	display:inline-block;
	vertical-align:bottom;
	transition:.1s linear all;
	transition:.1s linear all;
}

a.gl-3d span {
	position:relative;
	transition:.1s linear all;
	transition:.1s linear all;
	display:block;
	transform-origin:50% 0;
	transform:rotateX(0deg);
}

a.gl-3d:before {
	padding:0 5px;
	content:attr(title);
	position:absolute;
	left:0;
	top:0;
	background:#8e44ad;
	color:#fff;
	transition:.1s linear all;
	transition:.1s linear all;
	transform:rotateX(90deg);
	transform-origin:50% 100%;
}

a.gl-3d:hover span {
	transform:translateY(-50%) rotateX(90deg);
}

a.gl-3d:hover:before {
	transform:rotateX(0deg);
